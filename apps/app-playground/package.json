{
  "name": "@modrinth/app-playground",
  "scripts": {
    "build": "cargo build --release",
    "lint": "cargo fmt --check && cargo clippy --all-targets",
    "fix": "cargo clippy --all-targets --fix --allow-dirty && cargo fmt",
    "dev": "cargo run",
    "test": "cargo nextest run --all-targets --no-fail-fast"
  }
}
